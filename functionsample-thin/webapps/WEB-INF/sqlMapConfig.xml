<?xml version="1.0" encoding="UTF-8" ?>
<!DOCTYPE sqlMapConfig 
            PUBLIC "-//ibatis.apache.org//DTD SQL Map Config 2.0//EN"
            "http://ibatis.apache.org/dtd/sql-map-config-2.dtd">
<sqlMapConfig>
  <settings useStatementNamespaces="true"/>
  <!-- 共通 -->
  <sqlMap resource="sqlMap.xml" />
  <!-- ここから追加してください。 -->
  <!-- データベースアクセス機能用SQLマッピングファイル -->
  <sqlMap resource="databaseSqlMap.xml" />
  <sqlMap resource="uploadSqlMap.xml" />
  <sqlMap resource="pagelinkSqlMap.xml" />
  <sqlMap resource="transactionSqlMap.xml" />
  <sqlMap resource="dispatchSqlMap.xml" />
  <sqlMap resource="codelistSqlMap.xml" />
</sqlMapConfig>
